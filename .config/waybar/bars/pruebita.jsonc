{
  "output":"HDMI-A-1",
  "layer": "top",
  "height":30,
  "position": "top",
  "spacing": 4,
  "modules-left":[
    "hyprland/workspaces",
    "hyprland/window"
  ],
  "modules-center": [
    "clock"
  ],
  "modules-right":[
    "pulseaudio",
    "cpu",
    "memory",
    "custom/hyprshade",
    "tray",
    "custom/power"
  ],
  "include":[
    "$HOME/.config/waybar/modules/clock.jsonc",
    "$HOME/.config/waybar/modules/cpu.jsonc",
    "$HOME/.config/waybar/modules/memory.jsonc",
    "$HOME/.config/waybar/modules/power.jsonc",
    "$HOME/.config/waybar/modules/hyprshade.jsonc",
    "$HOME/.config/waybar/modules/pulseaudio.jsonc",
    "$HOME/.config/waybar/modules/hyprland-workspaces.jsonc",
    "$HOME/.config/waybar/modules/tray.jsonc",
    "$HOME/.config/waybar/modules/hyprland-window.jsonc"
  ]
}

